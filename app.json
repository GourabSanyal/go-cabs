{
  "expo": {
    "name": "GoCabs",
    "slug": "com.gocabs",
    "scheme": "solanaappkit",
    "version": "0.0.1",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "googleServicesFile": "./src/keys/google-services.json",
      "package": "com.gocabs",
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "ios": {
      "googleServicesFile": "./src/keys/GoogleService-Info.plist",
      "bundleIdentifier": "org.gocabs.com",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "We need access to your photos to let you pick images."
      }
    },
    "plugins": [
      "@react-native-google-signin/google-signin",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "We need access to your photos to let you pick images."
        }
      ],
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "YOUR_PROJECT_ID_HERE"
      }
    },
    "owner": "com.gocabs"
  }
} 